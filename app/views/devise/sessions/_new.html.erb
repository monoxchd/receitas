<div id='login'>
  <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
    <%= f.email_field(:email, size: 22, placeholder: 'Login') %>
    <%= f.password_field(:password, size: 22, placeholder: 'Senha') %>
    <%= link_to 'Esqueceu a sua senha?', new_password_path(resource_name) %>
    <br />
    <%= link_to 'Cadastre-se', new_registration_path(resource_name) %>
</div>
<% end %>
